# ==== 6-pin header J2 ====
# These four connections are shared with the FX2 connector
NET "EXIDI"  LOC = "A6" | IOSTANDARD = LVTTL  | SLEW = SLOW  | DRIVE = 6 ;
NET "EXICS"  LOC = "B6" | IOSTANDARD = LVTTL  | SLEW = SLOW  | DRIVE = 6 ;
NET "EXIDO"  LOC = "E7" | IOSTANDARD = LVTTL  | SLEW = SLOW  | DRIVE = 6 ;
NET "EXICLK"  CLOCK_DEDICATED_ROUTE = "FALSE" | LOC = "F7" | IOSTANDARD = LVTTL | PERIOD = 10ns HIGH 50%;
# ==== 6-pin header J4 ====
# These four connections are shared with the FX2 connector
NET "debug2<7>"  LOC = "D7" | IOSTANDARD = LVTTL  | SLEW = SLOW  | DRIVE = 6 ;
NET "debug2<6>"  LOC = "C7" | IOSTANDARD = LVTTL  | SLEW = SLOW  | DRIVE = 6 ;
NET "debug2<5>"  LOC = "F8" | IOSTANDARD = LVTTL  | SLEW = SLOW  | DRIVE = 6 ;
NET "debug2<4>"  LOC = "E8" | IOSTANDARD = LVTTL  | SLEW = SLOW  | DRIVE = 6 ;
# ==== 6-pin header J1 ====
# These four connections are shared with the FX2 connector
